<Window x:Class="DrillWpfObject.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DrillWpfObject"
        mc:Ignorable="d"
        Title="Register result" Height="200" Width="400">
    <StackPanel>
        <TextBlock Text="Distance (m)" />
        <TextBox Name="Distance" />
        <TextBlock Text="Name of runner" />
        <TextBox Name="NameOfRunner" />
        <TextBlock Text="Time (s)" />
        <TextBox Name="Time" />
        <Button Name="Save" Content="Save" Click="Save_Click" />
    </StackPanel>
</Window>
