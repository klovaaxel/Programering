        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DrillWpfHighScore"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel>
        <TextBlock Text="Namn" />
        <TextBox Name="PlayerName" />
        <TextBlock Text="Poäng" />
        <TextBox Name="Score" />
        <Button Name="SaveScore" Content="Save Score" Click="SaveScore_Click" />
        <TextBlock Name="HighScores" Text="High scores:" />
    </StackPanel>
</Window>
