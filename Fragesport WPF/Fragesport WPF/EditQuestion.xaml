<Window x:Class="Fragesport_WPF.EditQuestion"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Fragesport_WPF"
        mc:Ignorable="d"
        Title="Window1" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <TextBlock Text="Question"></TextBlock>
            <TextBox Name="QuestionBox" Height="50" Text="" ></TextBox>
            <TextBlock Text="Answer"></TextBlock>
            <TextBox Name="AnswerBox" Text="" Height="50"></TextBox>
            <Button Name="SaveEdit" Content="Save" Click="SaveEdit_Click"></Button>
            <StackPanel Name="QuestionList">

            </StackPanel>
        </StackPanel>
    </Grid>
</Window>
