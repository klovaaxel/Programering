<Window x:Class="Fragesport_WPF.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Fragesport_WPF"
        mc:Ignorable="d"
        Title="Admin Window" Height="450" Width="800">
    <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Height="50">
        <Button Name="EditQuestion" Content="Edit Question" Click="EditQuestion_Click"  Width="160" Margin="5"/>
        <Button Name="AddQuestionButton" Content="Add Question" Click="AddQuestion_Click"  Width="160" Margin="5" />
        <Button Name="UpdateDB" Content="UpdateDB" Click="UpdateDB_Click"  Width="160" Margin="5" />

    </StackPanel>
</Window>
